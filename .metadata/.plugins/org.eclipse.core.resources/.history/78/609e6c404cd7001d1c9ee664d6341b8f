--예약 등록 테이블
create table tbl_reserve_01 (
rno		 	char(8)	 	primary key
,rjumin	 	varchar2(13)
,rname	 	varchar2(20)
,rphone1 	char(3)
,rphone2 	char(4)
,rphone3 	char(4)
,remail	 	varchar2(30)
,rstat	 	char(1)
,rcode	 	char(4)
);

drop table tbl_reserve_01;

commit;
