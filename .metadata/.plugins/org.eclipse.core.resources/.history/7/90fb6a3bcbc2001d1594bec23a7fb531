package oop0315;

public class Test01_method {

	public static int test1(int a, int b) {
		int c=a+b;
		// return; 리턴 값이 없다
		return c; // 리턴 값은 1개만 리턴 가능
	} // test1 end
	
	public static void main(String[] args) {
		// 2. 리턴 값이 있는 경우
		
		System.out.println(Math.abs(-3));
		System.out.println(Math.max(5,7));
		
		test1(2,4);
	}

}
