package net.utility;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;

public class DBClose {
	//데이터베이스 연결 자원 반납
	
	public static void close(Connection con) {}//end
	public static void close(Connection con, PreparedStatement pstmt) {}//end
	public static void close(Connection con, PreparedStatement pstmt, ResultSet rs) {}//end

}//class end
